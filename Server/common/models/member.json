{
  "name": "Member",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "user_name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "required": false
    },
    "password": {
      "type": "string"
    },
    "role": {
      "type": "string"
    },
    "avatar_picture": {
      "type": "string"
    },
    "cover_picture": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "shopping-carts": {
      "type": "hasMany",
      "model": "ShoppingCart",
      "foreignKey": ""
    },
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "user_id"
    },
    "payments": {
      "type": "hasMany",
      "model": "Payment",
      "foreignKey": "user_id"
    },
    "notifications": {
      "type": "hasMany",
      "model": "Notification",
      "foreignKey": ""
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "user_id"
    },
    "products": {
      "type": "hasMany",
      "model": "Product",
      "foreignKey": "user_id"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
