{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "order_id": {
      "type": "string",
      "id": true,
      "generated": true
    },
    "order_time": {
      "type": "date",
      "default": "$now"
    },
    "quantity": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "shoppingCart": {
      "type": "belongsTo",
      "model": "ShoppingCart",
      "foreignKey": "shopping_cart_id"
    },
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "user_id"
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "product_id"
    },
    "factory": {
      "type": "belongsTo",
      "model": "Factory",
      "foreignKey": "factory_id"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
