{
  "name": "Product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "image": {
      "type": "string"
    },
    "time": {
      "type": "date"
    },
    "name": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "price": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "belongsTo",
      "model": "Category",
      "foreignKey": "category_id"
    },
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "user_id"
    },
    "post": {
      "type": "belongsTo",
      "model": "Post",
      "foreignKey": "post_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
